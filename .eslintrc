{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "ignorePatterns": ["temp.js", "**/node_modules/"],
  "rules": {
    "react/jsx-no-target-blank": 0,
    "react/prop-types": 0,
    "import/no-dynamic-require": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": true,
        "singleQuote": true
      }
    ]
  }
}